<Ui xmlns="http://www.blizzard.com/wow/ui/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.blizzard.com/wow/ui/"> 

	<Script file="LowHPPulser.lua" />

	<Frame name="LowHPPulser_JunkFrame" parent="UIParent" setAllPoints="true" hidden="false" enableMouse="false">
		<Scripts>
			<OnUpdate>LowHPPulser_OnUpdate();</OnUpdate>
		</Scripts>
	</Frame>

	<Frame name="LowHPPulser_LowHealthFrame" parent="UIParent" setAllPoints="true" hidden="true" enableMouse="false">
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\FullScreenTextures\LowHealth" alphaMode="ADD"/>
			</Layer>
		</Layers>
	</Frame>

	<Frame name="LowHPPulser_OutOfControlFrame" parent="UIParent" setAllPoints="true" hidden="true" enableMouse="false">
		<Layers>
			<Layer level="BACKGROUND">
				<Texture file="Interface\FullScreenTextures\OutOfControl" alphaMode="ADD"/>
			</Layer>
		</Layers>
	</Frame>

</Ui>
