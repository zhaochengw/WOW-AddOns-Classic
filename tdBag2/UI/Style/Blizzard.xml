            <Size x="33" y="33"/>
            <Anchors>
                <Anchor point="CENTER" x="0" y="0"/>
            </Anchors>
        </HighlightTexture>
        <Scripts>
            <OnMouseDown>
                self.texture:SetPoint('CENTER', 1, -1)
            </OnMouseDown>
            <OnMouseUp>
                self.texture:SetPoint('CENTER', 0, 0)
            </OnMouseUp>
        </Scripts>
