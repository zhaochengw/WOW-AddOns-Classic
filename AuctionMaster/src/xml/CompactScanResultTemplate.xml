<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\..\FrameXML\UI.xsd">

	<Frame name="CompactScanResultTemplate" virtual="true">
		<Size>
			<AbsDimension x="597" y="22"/>
		</Size>
		<Layers>
			<Layer level="BACKGROUND">
				<FontString name="$parentName" inherits="GameFontHighlightSmall" justifyH="LEFT">
					<Size>
						<AbsDimension x="190" y="22"/>
					</Size>
					<Anchors>
						<Anchor point="TOPLEFT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentCount" inherits="GameFontHighlightSmall" justifyH="CENTER">
					<Size>
						<AbsDimension x="45" y="22"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentName" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentOwner" inherits="GameFontHighlightSmall" justifyH="CENTER">
					<Size>
						<AbsDimension x="80" y="22"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentCount" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentBid" inherits="GameFontHighlightSmall" justifyH="RIGHT">
					<Size>
						<AbsDimension x="128" y="22"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentOwner" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
				<FontString name="$parentBuyout" inherits="GameFontHighlightSmall" justifyH="RIGHT">
					<Size>
						<AbsDimension x="145" y="22"/>
					</Size>
					<Anchors>
						<Anchor point="LEFT" relativeTo="$parentBid" relativePoint="RIGHT">
							<Offset>
								<AbsDimension x="0" y="0"/>
							</Offset>
						</Anchor>
					</Anchors>
				</FontString>
			</Layer>
		</Layers>
	</Frame>
</Ui>