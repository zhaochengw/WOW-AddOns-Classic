<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/">

	<Frame name="LunarSphereLoadFrame" movable="true" toplevel="true" hidden="true" parent="UIParent">
		<Size>
			<AbsDimension x="200" y="200"/>
		</Size>
		<Anchors>
			<Anchor point="CENTER">
				<Offset>
					<AbsDimension x="0" y="0"/>
				</Offset>
			</Anchor>
		</Anchors>
		<Scripts>
			<OnLoad inherit="prepend">
				OnLoadCheckMixin(self)
				LunarSphere_OnLoad(self);
			</OnLoad>
			<OnEvent>
				LunarSphere_OnEvent(self, event, arg1, arg2);
			</OnEvent>
		</Scripts>
	</Frame>
</Ui>
