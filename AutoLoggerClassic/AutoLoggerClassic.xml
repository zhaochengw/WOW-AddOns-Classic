<Ui>
    <FontString name="AutoLoggerClassicStringTemplate" font="Fonts/FRIZQT__.ttf" outline="NORMAL" justifyH="LEFT" virtual="true">
        <Color a="1" r="1" g="0.82" b="0" />
        <FontHeight>
            <AbsValue val="12" />
        </FontHeight>
    </FontString>
    <Frame name="AutoLoggerClassicFrame" parent="UIParent" inherits="BackdropTemplate" clampedToScreen="true" enableMouse="true" movable="true" frameStrata="DIALOG" hidden="true">
        <KeyValues>
            <KeyValue key="backdropInfo" value="BACKDROP_DIALOG_32_32" type="global"/>
            <KeyValue key="backdropBorderColor" value="LEGENDARY_ORANGE_COLOR" type="global"/>
            <KeyValue key="backdropBorderColorAlpha" value="1" type="number"/>
        </KeyValues>
        <Size x="660" y="690" />
        <Anchors>
            <Anchor point="CENTER" />
        </Anchors>
        <Frames>
            <Button name="$parentCloseButton" inherits="UIPanelCloseButton">
                <Anchors>
                    <Anchor point="TOPRIGHT" x="-8" y="-8" />
                </Anchors>
            </Button>
        </Frames>
        <Layers>
            <Layer level="OVERLAY">
                <Texture name="$parentTitleTexture" file="Interface/DialogFrame/UI-DialogBox-Header">
                    <Size x="300" y="62" />
                    <Anchors>
                        <Anchor point="TOP" x="0" y="12" />
                    </Anchors>
                </Texture>
                <FontString name="$parentTitleString" inherits="AutoLoggerClassicStringTemplate" text="AutoLoggerClassic">
                    <Anchors>
                        <Anchor point="TOP" x="0" y="-1" />
                    </Anchors>
                </FontString>
                <FontString name="$parentWOTLKRaidsString" inherits="AutoLoggerClassicStringTemplate" text="Wrath of the Lich King Raids">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="20" y="-20" />
                    </Anchors>
                </FontString>
                <FontString name="$parentWOTLKDungeonsString" inherits="AutoLoggerClassicStringTemplate" text="Wrath of the Lich King Dungeons (Heroics)">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="20" y="-126" />
                    </Anchors>
                </FontString>
                <FontString name="$parentTBCRaidsString" inherits="AutoLoggerClassicStringTemplate" text="The Burning Crusade Raids">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="20" y="-310" />
                    </Anchors>
                </FontString>
                <FontString name="$parentTBCDungeonsString" inherits="AutoLoggerClassicStringTemplate" text="The Burning Crusade Dungeons (Heroics)">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="20" y="-420" />
                    </Anchors>
                </FontString>
                <FontString name="$parentClassicRaidsString" inherits="AutoLoggerClassicStringTemplate" text="Classic Raids">
                    <Anchors>
                        <Anchor point="TOPLEFT" x="20" y="-605" />
                    </Anchors>
                </FontString>
            </Layer>
        </Layers>
        <Scripts>
            <OnLoad inherit="prepend">
                AutoLoggerClassic_OnLoad(self)
            </OnLoad>
            <OnDragStart>
                self:StartMoving()
            </OnDragStart>
            <OnDragStop>
                self:StopMovingOrSizing()
            </OnDragStop>
            <OnEvent>
                AutoLoggerClassic_OnEvent(self, event, ...)
            </OnEvent>
        </Scripts>
    </Frame>
</Ui>
